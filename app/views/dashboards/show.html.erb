<h1><%= current_user.username %>'s Dashboard</h1>

<div>
  <fieldset>
  <%= form_for @shout do |form| %>
    <div>
      <%= form.label :content %>
      <%= form.text_field :content %>
      <%= form.submit %>
    </div>
  <% end %>
  </fieldset>
</div>

<div>
  <%= render current_user.shouts %>
</div>
